{
    "name": "library-version",
    "displayName": "Library Version",
    "description": "this extension could show the vision of libraries which in node_modules, the version will be displayed in package.versions",
    "version": "0.1.0",
    "publisher": "momoko8443",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.showNpmPackageVersion"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
                "command": "extension.showNpmPackageVersion",
                "title": "Version: npm packages"
            }
        ],
        "languages": [{
            "id": "versions",
            "aliases": [
                "Versions"
            ],
            "extensions": [
                ".versions"
            ]
        }],
        "grammars": [{
            "language": "versions",
            "path": "./versions-syntax.json",
            "scopeName": "source.versions"
        }]
    },

    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}